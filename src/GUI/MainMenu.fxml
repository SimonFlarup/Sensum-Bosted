      <Label fx:id="nameUser" layoutX="116.0" layoutY="31.0" />
      <ListView fx:id="patientList" layoutX="300.0" layoutY="61.0" onMouseClicked="#selectPatient" prefHeight="318.0" prefWidth="276.0" />
